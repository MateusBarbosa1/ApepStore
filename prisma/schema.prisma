generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id     String @id @default(uuid())
  nome   String
  email  String @unique
  senha  String
}
model Produtos {
  id_produto     String @id @default(uuid())
  nome_produto   String
  descricao      String
  categoria      String
  preco          Decimal  @db.Decimal(10, 2)
  createdAt DateTime @default(now())
}
